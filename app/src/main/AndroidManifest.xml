<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cs990.restaurantbookingapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RestaurantBooking">
        <activity android:name=".profilePages.MyPaymentDetails"></activity>
        <activity android:name=".profilePages.MyRequests" />
        <activity android:name=".profilePages.MyRatings" />
        <activity android:name=".profilePages.MyBookings" />
        <activity android:name=".loginAndRegister.LoginActivity" />
        <activity android:name=".loginAndRegister.RegisterActivity" />
        <activity android:name=".restaurantPage.RestaurantPageActivity" />
        <activity android:name=".restaurantPage.BookingConfirmationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>